          }
        }
      }
    } else {
      Utils.markStageSkippedForConditional('BUILD: arm')
    }
  },

  'BUILD: Cortex-M': {
    if (!skip_ci && is_docs_only_build != 1) {
      node('CPU-SMALL') {
        ws("workspace/exec_${env.EXECUTOR_NUMBER}/tvm/build-cortexm") {
          init_git()
          docker_init(ci_cortexm)
          timeout(time: max_time, unit: 'MINUTES') {
