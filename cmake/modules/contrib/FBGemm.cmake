if(USE_FBGEMM)
  if(FBGEMM_PATH STREQUAL "")
    set(FBGEMM_PATH ${CMAKE_CURRENT_SOURCE_DIR}/FBGemm)
  endif()
  file(GLOB FBGEMM_CONTRIB_SRC src/contrib/fbgemm/*.cc)
  list(APPEND RUNTIME_SRCS ${FBGEMM_CONTRIB_SRC})
  include_directories(${FBGEMM_PATH}/include)
  find_library(FBGEMM_CONTRIB_LIB fbgemm  ${FBGEMM_PATH}/lib)
  find_library(FBGEMM_CPUINFO_CONTRIB_LIB cpuinfo ${FBGEMM_PATH}/lib)
  find_library(FBGEMM_CLOG_CONTRIB_LIB clog ${FBGEMM_PATH}/lib)
  find_library(FBGEMM_ASMJIT_CONTRIB_LIB asmjit ${FBGEMM_PATH}/lib)
  list(APPEND TVM_RUNTIME_LINKER_LIBS ${FBGEMM_CONTRIB_LIB})
  list(APPEND TVM_RUNTIME_LINKER_LIBS ${FBGEMM_CPUINFO_CONTRIB_LIB})
  list(APPEND TVM_RUNTIME_LINKER_LIBS ${FBGEMM_CLOG_CONTRIB_LIB})
  list(APPEND TVM_RUNTIME_LINKER_LIBS ${FBGEMM_ASMJIT_CONTRIB_LIB})
endif(USE_FBGEMM)
